<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Vivendi.UI</title>
</head>

<body>
    <header>
        <h1>
            <a asp-area=""
               asp-controller="Home"
               asp-action="Index"
            >
                Vivendi
            </a>
        </h1>

        <nav>
            <ul></ul>
        </nav>
    </header>

    <main role="main" class="pb-3">
        @RenderBody()
    </main>

    <footer class="border-top footer text-muted">
        <p>&copy; 2023 - Vivendi.UI</p>
    </footer>
</body>

</html>
